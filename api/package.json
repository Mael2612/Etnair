{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/api/server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config='./src/configs/jest.config.ts'",
    "seed:dev": "ts-node prisma/devSeed.ts",
    "seed:prod": "ts-node prisma/prodSeed.ts",
    "prebuild": "rimraf dist",
    "build:api": "tsc -p configs/ts/tsconfig.api.json",
    "build:prisma": "tsc -p configs/ts/tsconfig.prisma.json",
    "build:all": "tsc -p tsconfig.json",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:generate": "pnx prisma generate",
    "start:dev": "cross-env NODE_ENV=production npm run prebuild && npm run build:all && node dist/src/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "nodemon"
  },
